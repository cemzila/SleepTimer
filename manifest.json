{

    "manifest_version": 2,
    "name": "Uyku Alarmı",
    "version": "1.0",
  
    "description": "Uyku Saatini Kaçırmamak İçin Saat Kurun https://github.com/cemzila/SleepTimer",
    "homepage_url": "https://github.com/cemzila/SleepTimer",
    "icons": {
      "48": "icons/timer-48.png"
    },

    "content_scripts": [
      {
      "matches": ["<all_urls>"]
      }
    ],
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/timer-32.png",
      "default_title": "Uyku Alarmı",
      "default_popup": "popup/SleepTimer.html"
    }
  
  }
  
